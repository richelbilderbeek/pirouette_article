            <distribution id="BirthDeath.t:example_4_twin" spec="beast.evolution.speciation.BirthDeathGernhard08Model" birthDiffRate="@BDBirthRate.t:example_4_twin" relativeDeathRate="@BDDeathRate.t:example_4_twin" tree="@Tree.t:example_4_twin"/>
            <prior id="BirthRatePrior.t:example_4_twin" name="distribution" x="@BDBirthRate.t:example_4_twin">
