        <parameter id="rateAG.s:example_4_twin" lower="0.0" name="stateNode">1.0</parameter>
        <parameter id="rateAT.s:example_4_twin" lower="0.0" name="stateNode">1.0</parameter>
        <parameter id="rateCG.s:example_4_twin" lower="0.0" name="stateNode">1.0</parameter>
        <parameter id="rateGT.s:example_4_twin" lower="0.0" name="stateNode">1.0</parameter>
