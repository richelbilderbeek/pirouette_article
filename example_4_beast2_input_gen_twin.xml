    <operator id="ucldStdevScaler.c:example_4_twin" spec="ScaleOperator" parameter="@ucldStdev.c:example_4_twin" scaleFactor="0.5" weight="3.0"/>

    <operator id="CategoriesRandomWalk.c:example_4_twin" spec="IntRandomWalkOperator" parameter="@rateCategories.c:example_4_twin" weight="10.0" windowSize="1"/>

    <operator id="CategoriesSwapOperator.c:example_4_twin" spec="SwapOperator" intparameter="@rateCategories.c:example_4_twin" weight="10.0"/>

