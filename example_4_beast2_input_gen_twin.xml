                    <parameter id="gammaShape.s:example_4_twin" estimate="false" name="shape">1.0</parameter>
                    <parameter id="proportionInvariant.s:example_4_twin" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                    <substModel id="gtr.s:example_4_twin" spec="GTR" rateAC="@rateAC.s:example_4_twin" rateAG="@rateAG.s:example_4_twin" rateAT="@rateAT.s:example_4_twin" rateCG="@rateCG.s:example_4_twin" rateGT="@rateGT.s:example_4_twin">
                        <parameter id="rateCT.s:example_4_twin" estimate="false" lower="0.0" name="rateCT">1.0</parameter>
                        <frequencies id="estimatedFreqs.s:example_4_twin" spec="Frequencies" frequencies="@freqParameter.s:example_4_twin"/>
                    </substModel>
                </siteModel>
                <branchRateModel id="RelaxedClock.c:example_4_twin" spec="beast.evolution.branchratemodel.UCRelaxedClockModel" rateCategories="@rateCategories.c:example_4_twin" tree="@Tree.t:example_4_twin">
                    <LogNormal id="LogNormalDistributionModel.c:example_4_twin" S="@ucldStdev.c:example_4_twin" meanInRealSpace="true" name="distr">
                        <parameter id="RealParameter.12" estimate="false" lower="0.0" name="M" upper="1.0">1.0</parameter>
                    </LogNormal>
                    <parameter id="ucldMean.c:example_4_twin" estimate="false" name="clock.rate">1.0</parameter>
